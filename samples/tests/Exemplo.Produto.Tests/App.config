<?xml version="1.0"?>
<configuration>
  <configSections>
    <section name="frameworkConfiguration" type="Framework.Configuration.FrameworkConfigurationSection,Framework" allowLocation="true" allowDefinition="Everywhere"/>
  </configSections>
  <appSettings>
    <add key="Exemplo.Produto.Logger.LogLevel" value="Info"/>
  </appSettings>
  <connectionStrings>
    <add name="Exemplo.Produto" connectionString="server=.\sqlexpress;database=ExemploProduto;integrated security=true;" providerName="System.Data.SqlClient"/>
  </connectionStrings>
  <frameworkConfiguration>
    <daoFactory>
      <map interface="Exemplo.Produto.Data.IProdutoDAO" assembly="Exemplo.Produto.Data.Impl" typeName="Exemplo.Produto.Data.Impl.ProdutoDAO"/>
      <map interface="Exemplo.Produto.Data.IFornecedorDAO" assembly="Exemplo.Produto.Data.Impl" typeName="Exemplo.Produto.Data.Impl.FornecedorDAO"/>
      <map interface="Exemplo.Produto.Data.ICategoriaDAO" assembly="Exemplo.Produto.Data.Impl" typeName="Exemplo.Produto.Data.Impl.CategoriaDAO"/>
    </daoFactory>
    <resourceFile fullTypeName="Exemplo.Produto.Service.Resource"/>
    <loggers publish="logMessage, logReturn, logSQL, logEntry, logException">
      <!--<logger assembly="Framework" typeName="Framework.Log.DefaultLoggers.TextLogger" arguments="c:\temp\ExemploProduto.log" listen="logMessage, logReturn, logSQL, logEntry, logException"/>-->
    </loggers>
  </frameworkConfiguration>
<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/></startup></configuration>
